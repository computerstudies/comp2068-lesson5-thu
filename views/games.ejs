<% include partials/header.ejs %>

<h1><%= title %></h1>
<table class="table table-striped">
  <tr>
    <th>Title</th>
    <th>Publisher</th>
    <th>Genre</th>
    <th>Year</th>
  </tr>
  <% for (var i=0; i < games.length; i++) { %>
    <tr>
      <td><%= games[i].title %></td>
      <td><%= games[i].publisher %></td>
      <td><%= games[i].genre %></td>
      <td><%= games[i].year %></td>
    </tr>
  <% } %>
</table>
<% include partials/footer.ejs %>